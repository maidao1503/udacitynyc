{
	"name": "dataflowmaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_adl_empmaster",
						"type": "DatasetReference"
					},
					"name": "sourceempmaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "NYC_Payroll_EMP_MD",
						"type": "DatasetReference"
					},
					"name": "sinkpayrollemd"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceempmaster",
				"sourceempmaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EmployeeID,",
				"          LastName,",
				"          FirstName",
				"     )) ~> sinkpayrollemd"
			]
		}
	}
}