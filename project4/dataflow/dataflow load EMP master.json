{
	"name": "dataflow load EMP master",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_adl_empmaster",
						"type": "DatasetReference"
					},
					"name": "sourceEmpMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "NYC_Payroll_EMP_MD",
						"type": "DatasetReference"
					},
					"name": "sinkPayrollEmd"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceEmpMaster",
				"sourceEmpMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     partitionFileNames:['payroll_emp_md.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EmployeeID,",
				"          LastName,",
				"          FirstName",
				"     ),",
				"     partitionBy('hash', 1)) ~> sinkPayrollEmd"
			]
		}
	}
}